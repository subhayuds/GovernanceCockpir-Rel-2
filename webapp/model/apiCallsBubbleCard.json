{
	"_version": "1.14.0",
	"sap.app": {
		"id": "card.explorer.analytical.bubble.card",
		"type": "card",
		"title": "Sample of a Bubble Chart",
		"subTitle": "Sample of a Bubble Chart",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"shortTitle": "A short title for this Card",
		"info": "Additional information about this Card",
		"description": "A long description for this Card",
		"tags": {
			"keywords": [
				"Analytical",
				"Card",
				"Bubble",
				"Sample"
			]
		}
	},
	"sap.ui": {
		"technology": "UI5"
	},
	"sap.card": {
		"type": "Analytical",
		"header": {
			"title": "API calls in the platform"
		},
		"content": {
			"data": {
				"json": {},
				"path": "/d"
			},
			"chartType": "bubble",
			"chartProperties": {
				"title": {
					"visible": false
				},
				"legendGroup": {
					"layout": {
						"position": "right",
						"alignment": "topLeft"
					}
				}
			},
			"minHeight": "20rem",
			"dimensions": [
				{
					"name": "Sub-accounts",
					"value": "{subAccount}"
				}
			],
			"measures": [
				{
					"name": "API calls",
					"value": "{usage}"
				}
			],
			"feeds": [
				{
					"uid": "valueAxis",
					"type": "Measure",
					"values": [
						"API calls"
					]
				},
				{
					"uid": "valueAxis2",
					"type": "Measure",
					"values": [
						"API calls"
					]
				},
				{
					"uid": "bubbleWidth",
					"type": "Measure",
					"values": [
						"API calls"
					]
				},
				{
					"uid": "color",
					"type": "Dimension",
					"values": [
						"Sub-accounts"
					]
				}
			],
			"popover": {
				"active": true
			}
		}
	}
}