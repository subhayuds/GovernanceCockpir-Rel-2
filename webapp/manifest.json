{
  "_version": "1.65.0",
  "sap.app": {
    "id": "com.hcl.btp.governance.cockpit",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "2.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.16.3",
      "toolsId": "16778ccc-0f0b-49aa-8d0e-2227305dd145"
    },
    "crossNavigation": {
      "inbounds": {
        "BTPCockpit-manage": {
          "semanticObject": "BTPCockpit",
          "action": "manage",
          "title": "{{flpTitle}}",
          "subTitle": "{{flpSubtitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.132.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.hcl.btp.governance.cockpit.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "navContainerPages",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "com.hcl.btp.governance.cockpit.view",
        "async": true,
        "viewPath": "com.hcl.btp.governance.cockpit.view"
      },
      "routes": [
        {
          "name": "RoutePlatformDashboard",
          "pattern": ":?query:",
          "target": [
            "TargetPlatformDashboard"
          ]
        }
      ],
      "targets": {
        "TargetPlatformDashboard": {
          "id": "PlatformDashboard",
          "name": "PlatformDashboard"
        }
      }
    },
    "rootView": {
      "viewName": "com.hcl.btp.governance.cockpit.view.App",
      "type": "XML",
      "id": "App"
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "comhclbtpgovernancecockpit"
  }
}
